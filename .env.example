# Example .env file for Aggregator and Token Prices CLI

# GCP Configuration
TF_VAR_project_id=your-gcp-project-id
TF_VAR_region=us-central1
TF_VAR_bucket_name=aggregator-data
GOOGLE_APPLICATION_CREDENTIALS=path/to/service-account.json

# ClickHouse Configuration
CLICKHOUSE_HOST=your-clickhouse-host
CLICKHOUSE_PORT=9440
CLICKHOUSE_USER=default
CLICKHOUSE_PASSWORD=your-clickhouse-password

# CoinGecko Configuration
COINGECKO_API_KEY=your-coingecko-api-key

# Application Configuration
LOG_LEVEL=DEBUG

# Terraform-Specific Variables
TF_VAR_clickhouse_host=your-clickhouse-host
TF_VAR_clickhouse_port=9440
TF_VAR_clickhouse_user=default
TF_VAR_clickhouse_password=your-clickhouse-password
TF_VAR_coingecko_api_key=your-coingecko-api-key
